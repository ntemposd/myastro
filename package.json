{
  "name": "myastro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "sync:experience": "tsx scripts/sync-experience.ts",
    "sync:posts": "tsx scripts/sync-posts.ts",
    "sync:projects": "tsx scripts/sync-projects.ts",
    "sync:content": "npm run sync:experience && npm run sync:posts && npm run sync:projects",
    "dev": "npm run sync:content && astro dev",
    "build": "npm run sync:content && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "astro": "^5.14.1",
    "dotenv": "^17.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6"
  },
  "dependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@notionhq/client": "^5.1.0",
    "notion-to-md": "^3.1.9"
  }
}
